{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "VSContext": "Data Source=VUAContext.db"
  },
  "AllowedHosts": "*",
  "FFmpeg": {
    "ExecutablesPath": "C:\\FFmpeg\\bin"
  },
  "Video": {
    "DurationInSeconds": 1,
    "VideoWidth": 1920,
    "VideoHeight": 1080,
    "OutputDirectory": "wwwroot/video",
    "AutoDelete": true
  },
  "Images": {
    "ImageWidth": 1920,
    "ImageHeight": 1080,
    "BlockSize": 20,
    "OutputDirectory": "wwwroot/img",
    "AutoDelete": true,
    "ImageType": "Binary"
  },
  "Queries": {
    "EOQMarker": "]",
    "RemovalThresholdInSeconds": 15
  }
}
